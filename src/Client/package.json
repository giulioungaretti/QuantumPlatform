{
  "name": "qanumplatform",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "electronWebpack": {
    "title": true,
    "main": {
      "sourceDirectory": "src/Main",
      "webpackConfig": "webpack.additions.main.js"
    },
    "renderer": {
      "sourceDirectory": "src/Renderer",
      "webpackConfig": "webpack.additions.renderer.js"
    }
  },
  "dependencies": {
    "source-map-support": "^0.5",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "bulma": "^0.7.4",
    "bulma-pageloader": "^2.1.0",
    "react": "^16.8",
    "react-dom": "^16.8"
  },
  "devDependencies": {
    "electron": "^7.2",
    "electron-builder": "^20.39",
    "electron-devtools-installer": "^2.2",
    "electron-webpack": "^2.6",
    "electron-window-state": "^5.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/runtime": "^7.2.0",
    "babel-loader": "^8.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.1",
    "fable-compiler": "^2.2.3",
    "fable-loader": "^2.1.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "remotedev": "^0.2.9",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "whatwg-fetch": "^3.0.0"
  }
}
